{
    "Darwin": {
        "brew": [
            "/bin/bash -c '$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/HEAD/install.sh)'"
        ],
        "jq": [
            "brew install jq"
        ]
    },
    "Linux": {
        "jq": [
            "sudo apt-get install -y jq"
        ],
        "gcc": [
            "sudo apt-get update; sudo apt-get install -y build-essential;"
        ]
    }
}